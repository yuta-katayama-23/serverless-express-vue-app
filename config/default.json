{
	"auth": {
		"issure": "https://accounts.google.com",
		"scopes": ["openid", "email", "profile"]
	},
	"dynamodb": {
		"session": {
			"name": "slsexp.sid",
			"resave": false,
			"saveUninitialized": false,
			"cookie": {
				"maxAge": 604800000,
				"httpOnly": true,
				"secure": true
			}
		}
	},
	"sequelize": {
		"base": {
			"database": "sls-express",
			"dialect": "mysql",
			"timezone": "+09:00",
			"pool": { "max": 5, "min": 1, "acquire": 30000, "idle": 10000 }
		},
		"local": {
			"username": "root",
			"password": ""
		}
	}
}
